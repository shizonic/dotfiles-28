Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

URxvt.depth: 32
URxvt.geometry: 90x28
URxvt.font: xft:Terminus:style=Regular:pixelsize=18
URxvt.boldFont: xft:Terminus:style=Regular:pixelsize=18
URxvt.italicFont: xft:Terminus:style=Regular:pixelsize=18
URxvt.boldItalicFont: xft:Terminus:style=Regular:pixelsize=18
URxvt.scrollBar: false
URxvt.saveLines: 100000
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
!URxvt.letterSpace: -1
!urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800

!xterm*faceName: Terminus:style=Regular:pixelsize=12
!xterm*faceName: Inconsolata::style=Regular:pixelsize=18
!xterm*boldFont: Inconsolata::style=Regular:pixelsize=18
xterm*faceName: Hack:style=Regular:pixelsize=16
xterm*boldFont: Hack:style=Regular:pixelsize=16
xterm*allowBoldFonts: false
!xterm*vt100*geometry: 110x36
xterm*saveLines: 100000
xterm*background: #232323
xterm*foreground: #ffffff
xterm*geometry: 84x26

! irssi fix
xterm*eightBitInput: false
xterm*metaSendsEscape: true

!# rofi
rofi.font: Terminus 12
rofi.theme: android_notification

!*colorIT:                             #E3AB8D
!*colorBD:                             #F0E5DF
!*colorUL:                             #cd546c
!*VT100.colorBDMode:                   true
!*VT100.colorBD:                       red
!*VT100.colorULMode:                   true
!*VT100.colorUL:                       magenta
!*VT100.veryBoldColors:                6

!special
*background: #262729
*foreground: #CECECE
*cursorColor: #a0a0a0

!black
*color0: #25374A
*color8: #3A526B

!red
*color1: #BD4A4A
*color9: #CC6666

!green
*color2: #B7CE42
*color10: #BDE077

!yellow
*color3: #FFF872
*color11: #FFFCC1

!blue
*color4: #188ED8
*color12: #73B6D0

!magenta
*color5: #F26B9E
*color13: #ff5879

!cyan
*color6: #72AD8C
*color14: #9FE3BC

!white
*color7: #CECECE
*color15: #FAFAFA
